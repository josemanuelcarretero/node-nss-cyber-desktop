{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
      "prettier",
      "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-console": 1,
      "prettier/prettier": 2
    }
}
